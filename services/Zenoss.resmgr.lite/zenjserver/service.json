{
    "Description": "Enables the dynamic view of organizers",
    "Command": "su - zenoss -c \"/opt/zenoss/bin/zenjserver run -c --duallog \"",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660",
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}"
        },
        "/opt/zenoss/etc/zenjserver.conf": {
            "FileName": "/opt/zenoss/etc/zenjserver.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0664"
        }
    },
    "Endpoints": [
        {
            "Name": "zproxy",
            "Application": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "zenhub",
            "Application": "localhost_zenhubPB",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "redis",
            "Application": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "controlplane_consumer",
            "Application": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "dynamicserver",
            "Application": "dynamicserver",
            "PortNumber": 8700,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1
    },
    "RAMCommitment": "256M",
    "CPUCommitment": 1,
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "pythondaemon"
            ],
            "path": "/opt/zenoss/log/zenjserver.log",
            "type": "zenjserver"
        },
        {
            "filters": [
                "supervisord"
            ],
            "path": "/opt/zenoss/log/audit.log",
            "type": "zenossaudit"
        }
    ],
    "Name": "zenjserver",
    "Services": [],
    "Tags": [
        "daemon",
        "collector"
    ],
    "HealthChecks": {
        "heartbeat": {
            "Script": "echo heartbeat",
            "Interval": 5.0
        }
    },
    "Volumes": [
        {
            "Owner": "zenoss:zenoss",
            "ContainerPath": "/home/zenoss/.ssh",
            "Type": "",
            "ResourcePath": ".ssh",
            "Permission": "0755"
        },
        {
            "Owner": "zenoss:zenoss",
            "ResourcePath": "var-zenpacks",
            "ContainerPath": "/var/zenoss",
            "Permission": "0775"
        }
    ]
}
