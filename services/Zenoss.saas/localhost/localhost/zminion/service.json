{
    "Description": "Performs distributed bash executions",
    "Command": "su - zenoss -c '/opt/zenoss/bin/zminion --minion-name zminion_{{(parent .).Name}} serve'",
    "Endpoints": [
        {
            "Name": "zproxy",
            "Application": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "zenhub",
            "Application": "{{(parent (parent .)).Name}}_zenhubPB",
            "PortNumber": 8789,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "redis",
            "Application": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Name": "controlplane_consumer",
            "Application": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1,
        "max": 1
    },
    "RAMCommitment": "256M",
    "CPUCommitment": 1,
    "Launch": "auto",
    "Name": "zminion",
    "Tags": [
        "daemon",
        "collector"
    ],
    "HealthChecks": {
        "redis_answering": {
            "Script": "/opt/zenoss/bin/healthchecks/redis_answering",
            "Interval": 5.0
        }
    },
    "Volumes": []
}
