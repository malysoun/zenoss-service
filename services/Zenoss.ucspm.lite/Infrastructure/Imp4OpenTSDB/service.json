{
    "Name": "Imp4OpenTSDB",
    "Description": "OpenTSDB and perf data import worker for import4",
    "Launch": "manual",
    "Command": "bash -c \"/import4/pkg/bin/imp4opentsdb.sh\"",
    "Services": [],
    "Tags": [
        "daemon"
    ],
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "Min": 1,
        "Max": 0,
        "Default": 6
    },
    "ChangeOptions": null,
    "HostPolicy": "",
    "Hostname": "",
    "Privileged": false,
    "Context": null,
    "Endpoints": [
     {
           "Name": "opentsdb-writer",
           "Application": "opentsdb-writer",
           "PortNumber": 4242,
           "Protocol": "tcp",
           "Purpose": "import"
     }
   ],
   "Tasks": null,
   "LogFilters": null,
   "Volumes": [
     {
       "Owner": "zenoss:zenoss",
       "Permission": "0777",
       "ResourcePath": "import4",
       "ContainerPath": "/import4"
     }
    ],
   "LogConfigs": [
     {
       "Path": "/opt/zenoss/log/imp4_opentsdb.log",
       "Type": "opentsdb-reader",
       "Filters": null,
       "LogTags": null
     }
   ],
   "Snapshot": {
     "Pause": "",
     "Resume": ""
   },
   "RAMCommitment": "128M",
   "CPUCommitment": 1,
   "Commands": null,
   "Actions": null,
   "HealthChecks": {
     "Script_OK": {
       "Script": "[ 1 -le $(pgrep -fla '/bin/bash /import4/pkg/bin/imp4opentsdb.sh' | wc -l) ]",
       "Interval": 15,
       "Timeout": 0
     }
   },
   "Prereqs": [
     {
       "Name": "DMD UUID obtained",
       "Script": "[ -f /import4/dmd_uuid.txt ]"
     },
     {
       "Name": "imp4opentsdb script",
       "Script": "[ -f /import4/pkg/bin/imp4opentsdb.sh ]"
     }
   ],
   "MonitoringProfile": {
     "MetricConfigs": null,
     "GraphConfigs": null,
     "ThresholdConfigs": null
   },
   "MemoryLimit": 0,
   "CPUShares": 0,
   "PIDFile": ""
}
