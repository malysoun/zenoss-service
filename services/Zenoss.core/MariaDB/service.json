{
    "CPUCommitment": 2,
    "Command": "/usr/bin/mysqld_safe",
    "Description": "MariaDB database server",
    "Endpoints": [
        {
            "Application": "zodb_mariadb",
            "Name": "mariadb",
            "PortNumber": 3306,
            "Protocol": "tcp",
            "Purpose": "export"
        }
    ],
    "HealthChecks": {
        "answering": {
            "Interval": 5.0,
            "Script": "mysql --protocol TCP -uroot -hlocalhost -P3306 -e 'select 1' > /dev/null"
        }
    },
    "ImageID": "zenoss/zenoss5x",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "path": "/var/log/mysqld.log",
            "type": "mariadb"
        }
    ],
    "Name": "MariaDB",
    "PIDFile": "exec echo /var/lib/mysql/$(hostname).pid",
    "RAMCommitment": "1G",
    "Runs": {
        "rebuild_zodb_session": "${ZENHOME:-/opt/zenoss}/bin/zenrun mysql.sh rebuild_zodb_session"
    },
    "Snapshot": {
        "Pause": "PAUSE_CHECK_TIMEOUT=60 LOCK_HOLD_DURATION=600 ${ZENHOME:-/opt/zenoss}/bin/quiesce-mariadb.sh pause",
        "Resume": "${ZENHOME:-/opt/zenoss}/bin/quiesce-mariadb.sh resume"
    },
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxr-xr-x 12 mysql mysql 4096 Feb 25 21:27 /var/lib/mysql/",
            "ContainerPath": "/var/lib/mysql",
            "Owner": "mysql:mysql",
            "Permission": "0755",
            "ResourcePath": "mariadb"
        }
    ]
}